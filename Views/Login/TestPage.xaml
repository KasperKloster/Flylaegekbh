<UserControl x:Class="FlyveLægeKBH.Views.Login.TestPage"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:FlyveLægeKBH.Views.Login"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <StackPanel>
            <TextBox Style="{StaticResource searchBar}" x:Name="Get_all_info_test_tbx" Margin="0,10"/>
            <Button Style="{StaticResource submitBtn}" x:Name="Get_all_info_test_btn" Content="Get all info test" Click="Get_all_info_test_btn_Click"/>
            <TextBlock Style="{StaticResource headline2}" x:Name="Get_all_info_test_card"/>
        </StackPanel>
        
    </Grid>
</UserControl>
